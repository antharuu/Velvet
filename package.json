{
	"name": "velvet-lang",
	"description": "Velvet is a HTML preprocessor, converting Velvet language (PUGjs like) to HTML.",
	"keywords": [
		"velvet",
		"preprocessor",
		"HTML",
		"front-end",
		"PUGjs"
	],
	"version": "0.1.7",
	"main": "./lib/index.js",
	"type": "module",
	"types": "./lib",
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc",
		"preview": "vite preview",
		"test": "npx vitest --globals --sequence.shuffle --sequence.shuffle",
		"test:ui": "npm run test -- --ui",
		"test:cover": "npm run test -- --coverage",
		"prod": "release-it --verbose",
		"lint": "eslint src --fix",
		"clean": "rm -rf node_modules"
	},
	"devDependencies": {
		"@types/jest": "^29.1.0",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"@vitest/coverage-c8": "^0.23.4",
		"@vitest/ui": "^0.23.4",
		"c8": "^7.12.0",
		"eslint": "^8.24.0",
		"release-it": "^15.4.2",
		"typescript": "^4.8.4",
		"vite": "^3.1.4",
		"vitest": "^0.23.4"
	}
}
