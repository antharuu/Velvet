{
	"name": "velvet-lang",
	"description": "Velvet is a HTML preprocessor, converting Velvet language (PUGjs like) to HTML.",
	"version": "0.1.3",
	"main": "./lib/index.js",
	"type": "module",
	"types": "./lib",
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc",
		"preview": "vite preview",
		"test": "vitest --globals --sequence.shuffle --sequence.shuffle",
		"test:ui": "npx vitest --globals --ui --sequence.shuffle --sequence.shuffle",
		"test:cover": "npx vitest --globals --coverage --sequence.shuffle --sequence.shuffle",
		"prod": "release-it",
		"clean": "rm -rf node_modules"
	},
	"devDependencies": {
		"@types/jest": "^29.0.3",
		"@vitest/coverage-c8": "^0.23.4",
		"@vitest/ui": "^0.23.4",
		"c8": "^7.12.0",
		"release-it": "^15.4.2",
		"typescript": "^4.8.4",
		"vite": "^3.1.4",
		"vitest": "^0.23.4"
	}
}
